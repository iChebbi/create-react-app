{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "semi":["error","never"],
    "func-names": ["error", "never"]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": ["node_modules", "src/"]
      }
    }
  },
  "env": {
    "browser": true,
    "node": true
  }
}